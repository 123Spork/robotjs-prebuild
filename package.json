{
  "name": "@todesktop/robotjs-prebuild",
  "version": "0.6.4",
  "description": "Node.js Desktop Automation.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "run-script-os",
    "test:darwin:linux": "jasmine test/**/*.js",
    "test-keyboard": "node test/keyboard.js",
    "test:win32": "jasmine test/**/*.js",
    "install": "prebuild-install --runtime electron || node-gyp rebuild",
    "install-debug": "prebuild-install || node-gyp rebuild --debug",
    "prebuild": "prebuild -r electron -t 97 -t 98 -t 99",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ToDesktop/robotjs-prebuild.git"
  },
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/ToDesktop/robotjs-prebuild/issues"
  },
  "homepage": "https://github.com/ToDesktop/robotjs-prebuild",
  "dependencies": {
    "nan": "^2.14.2",
    "prebuild-install": "^7.0.0"
  },
  "devDependencies": {
    "jasmine": "^2.99.0",
    "prebuild": "^11.0.0",
    "run-script-os": "^1.0.3",
    "tape": "^4.8.0",
    "targetpractice": "0.0.7"
  }
}
