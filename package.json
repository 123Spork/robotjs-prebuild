{
  "name": "@todesktop/robotjs-prebuild",
  "version": "0.6.7",
  "description": "Node.js Desktop Automation.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "run-script-os",
    "test:darwin:linux": "jasmine test/**/*.js",
    "test-keyboard": "node test/keyboard.js",
    "test:win32": "jasmine test/**/*.js",
    "install": "prebuild-install --verbose --runtime electron || node-gyp rebuild",
    "install-debug": "prebuild-install --verbose || node-gyp rebuild --debug",
    "prebuild": "prebuild -r electron -t 97 -t 98 -t 99 -t 101 -t 103 -t 106 -t 107 --debug",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "publish": "npm publish",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ToDesktop/robotjs-prebuild.git"
  },
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/ToDesktop/robotjs-prebuild/issues"
  },
  "homepage": "https://github.com/ToDesktop/robotjs-prebuild",
  "dependencies": {
    "nan": "^2.16.0",
    "node-addon-api": "^5.0.0",
    "patch-package": "^6.4.7",
    "prebuild-install": "^7.1.1"
  },
  "devDependencies": {
    "electron": "^20.0.0",
    "electron-rebuild": "^3.2.9",
    "node-gyp": "^9.1.0",
    "prebuild": "^11.0.4"
  },
  "overrides": {
    "node-gyp": "^9.1.0",
    "prebuild": "^11.0.4"
  }
}
